{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components": [
        "./app/components"
      ],
      "@config": [
        "./app/config"
      ],
      "@i18n": [
        "./app/i18n"
      ],
      "@models": [
        "./app/models"
      ],
      "@models/*": [
        "./app/models/*"
      ],
      "@navigators": [
        "./app/navigators"
      ],
      "@screens": [
        "./app/screens"
      ],
      "@theme": [
        "./app/theme"
      ],
      "@services/*": [
        "./app/services/*"
      ],
      "@utils/*": [
        "./app/utils/*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    },
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "target": "esnext",
    "lib": [
      "esnext",
      "dom"
    ],
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "index.js",
    "App.js",
    "app",
    "test"
  ],
  "extends": "expo/tsconfig.base"
}